{
  "schemaVersion": 1,
  "id": "chatutilities",
  "version": "${version}",

  "name": "Chat Utilities",
  "description": "Adds a Time Stamp to the chat messages, and more!",
  "authors": [
    "Francy-Chan",
    "omoflop"
  ],
  "contact": {
    "homepage": "https://github.com/Francy-Chan/ChatTimeStamp/",
    "sources": "https://github.com/Francy-Chan/ChatTimeStamp/",
    "issues": "https://github.com/Francy-Chan/ChatTimeStamp/issues"
  },

  "license": "LGPL-2.1",
  "icon": "assets/chatutilities/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "org.moon.chatutilities.ChatUtilities"
    ],
    "modmenu": [
      "org.moon.chatutilities.config.ModMenuConfig"
    ]
  },
  "mixins": [
    "chatutilities.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.5",
    "minecraft": ">=1.18-",
    "java": ">=16"
  },
  "custom": {}
}
